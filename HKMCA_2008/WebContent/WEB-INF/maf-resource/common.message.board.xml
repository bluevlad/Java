<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE maf-resource PUBLIC "-//UBQ//DTD maf-command config XML V1.0//EN" "resourceBundle.dtd">

<bundle defaultLocale="en">
    <resource key="insert.ok" type="text">
        <message locale="en" value="Registered." />
        <message locale="ko" value="등록되었습니다." />
    </resource>
    <resource key="insert.fail" type="text">
        <message locale="en" value="Registration could not be completed." />
        <message locale="ko" value="등록중 오류가 발생하였습니다." />
    </resource>
    <resource key="update.ok" type="text">
        <message locale="en" value="Modified." />
        <message locale="ko" value="수정되었습니다." />
    </resource>  
    <resource key="update.fail" type="text">
        <message locale="en" value="could not be modified." />
        <message locale="ko" value="수정중 오류가 발생했습니다." />
    </resource>
    <resource key="delete.ok" type="text">
        <message locale="en" value="record(s) deleted." />
        <message locale="ko" value="정상적으로  삭제되었습니다." />
    </resource>
    <resource key="delete.fail" type="text">
        <message locale="en" value="Deletion could not be completed." />
        <message locale="ko" value="삭제중 오류가 발생하였습니다." />
    </resource>
    <resource key="invalid.op" type="text">
        <message locale="en" value="Invalid Operation" />
        <message locale="ko" value="잘못된 요청입니다" />
    </resource> 
    <resource key="list.error" type="text">
        <message locale="ko" value="게시판 목록 가져오는중 오류가 발생하였습니다." />
    </resource>
    <resource key="message.reply_fail" type="text">
        <message locale="en" value="Answer/Reply/Feedback has not been registered." />
        <message locale="ko" value="답변이 등록되지 못하였습니다." />
    </resource>
    <resource key="message.reply_ok" type="text">
        <message locale="en" value="Answer/Reply/Feedback has been registered." />
        <message locale="ko" value="답변이 등록되었습니다." />
    </resource>
    <resource key="comment.added" type="text">
        <message locale="en" value="Comment added" />
        <message locale="ko" value="코멘트가 등록되었습니다" />
    </resource>
    <resource key="comment.deleted" type="text">
        <message locale="en" value="Comment deleted" />
        <message locale="ko" value="코멘트가 삭제되었습니다" />
    </resource>
</bundle>