<?xml version="1.0" encoding="UTF-8"  ?>
<!DOCTYPE maf-command PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "excel-upload.dtd">
<upload desc="upload 수강신청 등록" startrow="1">
	<sqls>
		<sql seq="0">
			{call SP_WCC_LEC_REQ_BATCH_INSERT(:usn, :lec_cd, :lec_num, :req_stat, :update_id)}
		</sql>
	</sqls>
	<fields>
		<field col="0" fieldname="usn" title="User ID" sample="admin" desc="User ID" />
		<field col="1" fieldname="lec_cd" title="Lecture Code" sample="PRF_003-R-2009_25" desc="Lecture Code" />
        <field col="2" fieldname="lec_num" title="Lecture Number" sample="1" desc="Lecture Number" />
        <field col="3" fieldname="req_stat" title="Request State" sample="LR" desc="Request State" />
        <field col="4" fieldname="update_id" title="Update User" sample="admin" desc="Update User" />
	</fields>
</upload>