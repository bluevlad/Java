         "excel-upload.dtd">
<upload desc="eTest 문제은행 등록" startrow="1">
	<sqls>
		<sql seq="0">
		 INSERT INTO exm_bnk (queid,
             qweowner, quetitle, quetext, queviw1, queviw2, queviw3,
             queviw4, queviw5, queviw6, queviw7, queviw8, queviw9, queviw10, 
             queviw11, queviw12, queviw13, queviw14, queviw15, queviw16, queviw17, 
             queviw18, queviw19, queviw20, quecount,
             quedesc, queansw, quescore, quetype, quelevel,  cat_id,
             reg_usn, update_usn,ACTIVE_YN
            )
     VALUES (to_char(sysdate,'DD') || lpad( seq_exm_bnk_id.NEXTVAL,10,0), 
     		 :qweowner, :quetitle, :quetext, :queviw1, :queviw2, :queviw3,
             :queviw4, :queviw5, :queviw6, :queviw7, :queviw8, :queviw9, :queviw10, 
             :queviw11, :queviw12, :queviw13, :queviw14, :queviw15, :queviw16, :queviw17, 
             :queviw18, :queviw19, :queviw20, :quecount,
             :quedesc, :queansw, :quescore, :quetype, :quelevel,  :cat_id,
             :reg_usn, :reg_usn,'Y'
            )
		</sql>
	</sqls>
	<fields>
		<field col="0" fieldname="cat_id" title="cat_id" type="string" check="true" sample="101" desc="Category ID" />
		<field col="1" fieldname="quetitle" title="quetitle" type="string" check="true" sample="What happens to the voltage output of the map sensor during acceleration?" desc="Queue Title" />
		<field col="2" fieldname="quetext" title="quetext" type="string" check="true" sample="" desc="View" />
		
		<field col="3" fieldname="quecount" title="quecount" type="string" check="true" sample="4" desc="Queue COunt" />
		<field col="4" fieldname="queansw" title="queansw" type="string" check="true" sample="2#3" desc="Answer" />
		<field col="5" fieldname="quescore" title="quescore" type="string" check="true" sample="1" desc="Score" />
		<field col="6" fieldname="quetype" title="quetype" type="string" check="true" sample="s" desc="Type[s:Single choice, m:Mulitple choice, t:Short answer, s:Essay type]" />
		<field col="7" fieldname="quelevel" title="quelevel" type="string" check="true" sample="3" desc="Difficult[5:Very Difficult 4:Difficult 3:Normal 2:Easy 1 : Very Easy]" />
		
		<field col="8" fieldname="queviw1" title="queviw1" type="string" sample="Decreased" desc="View 1" />
