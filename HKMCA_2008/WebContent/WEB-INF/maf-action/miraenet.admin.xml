<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-command PUBLIC
         "-//Miraenet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
	<global-view></global-view>
    
<action uri="codeMng.do" type="miraenet.app.codemanager.CodeMngAction" desc="코드관리" title="코드관리"
    jsppath="/WEB-INF/jsp/mafapp/codemanager/" default="list">
    <action-view>
        <view-info name="list" uri="list.jsp" useTemplate="www" />
        <view-info name="edit" uri="edit.jsp" useTemplate="www" />
    </action-view>
    <cmds>
        <cmd name="list" method="cmdList" formname="frm1" />
        <cmd name="add" method="cmdAdd" formname="frm1" />
        <cmd name="edit" method="cmdEdit" formname="frm1" />
        <cmd name="insert" method="cmdInsert" />
        <cmd name="update" method="cmdUpdate" />
        <cmd name="delete" method="cmdDelete" />
        <cmd name="ginsert" method="cmdGInsert" />
    </cmds>
    <forms>
        <form name="frm1" bundle="common">
            <input name="group_cd" message="group_cd" type="text"></input>
            <input name="group_name" message="group_nm" type="text"></input>
            <input name="code_cd" message="code_cd" type="text"></input>
            <input name="code_name" message="code_nm" type="text"></input>
        </form>
    </forms>
</action>
	
<action uri="MenuAdmin.do" type="miraenet.app.siteManager.MenuAdmin" title="메뉴관리" desc="메뉴관리"
	jsppath="/WEB-INF/jsp/mafapp/menuadmin/" default="default" debug="true">
	<action-view>
		<view-info name="default" uri="menu_admin.jsp" useTemplate="www" />
		<view-info name="tree" uri="menu_tree.jsp" useTemplate="inner_frame" />
		<view-info name="edit" uri="menu_edit.jsp" useTemplate="inner_frame" />
		<view-info name="write" uri="menu_edit.jsp" useTemplate="inner_frame" />
	</action-view>
	<cmds>
		<cmd name="default" method="cmdDefault" authtype="R" />
		<cmd name="tree" method="cmdTree" authtype="R" />
		<cmd name="add" method="cmdAdd" authtype="C" />
		<cmd name="edit" method="cmdEdit" authtype="U" />
		<cmd name="update" method="cmdUpdate" authtype="U" />
		<cmd name="insert" method="cmdInsert" authtype="C" />
		<cmd name="delete" method="cmdDelete" authtype="D" />
	</cmds>
</action>
  
    <action uri="SiteAdmin.do" type="miraenet.app.siteManager.MafSiteAction" desc="사이트관리" title="사이트관리"
     jsppath="/WEB-INF/jsp/mafapp/sitemanager/site/" default="default" debug="true">
        <action-view>
            <view-info name="default" uri="default.jsp" useTemplate="www" />
            <view-info name="edit" uri="edit.jsp" useTemplate="www" />
            <view-info name="add" uri="edit.jsp" useTemplate="www" />
        </action-view>
        <cmds>
            <cmd name="default" method="cmdDefault" formname="frm1" authtype="R" />
            <cmd name="edit" method="cmdEdit" formname="frm1" authtype="C" />
            <cmd name="add" method="cmdAdd" formname="frm1" authtype="C" />
            <cmd name="update" method="cmdUpdate" />
            <cmd name="insert" method="cmdInsert" />
            <cmd name="delete" method="cmdDelete" />
        </cmds>
    </action>
        
<action uri="holiday.do" type="miraenet.app.siteManager.HolidayAction" desc="휴일관리" title="휴일관리"
    jsppath="/WEB-INF/jsp/mafapp/sitemanager/holiday/" default="list">
	<action-view>
		<view-info name="list" uri="list.jsp" useTemplate="inner" />
		<view-info name="edit" uri="edit.jsp" useTemplate="inner" />
	</action-view>
	<cmds>
		<cmd name="list" method="cmdList" />
		<cmd name="edit" method="cmdEdit" />
		<cmd name="update" method="cmdUpdate" />
		<cmd name="insert" method="cmdInsert" />
		<cmd name="add" method="cmdAdd" />
		<cmd name="delete" method="cmdDelete" />
	</cmds>
</action>
	
<action uri="LangManager.do" type="miraenet.app.siteManager.LangManagerAction" desc="사용언어 관리" title="사용언어 관리"
    jsppath="/WEB-INF/jsp/mafapp/langmanager/" default="list">
	<action-view>
		<view-info name="list" uri="list.jsp" useTemplate="www" />
	</action-view>
	<cmds>
		<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
		<cmd name="update" method="cmdUpdate" formname="frm1" authtype="U" />
	</cmds>
	<forms>
		<form name="frm1" bundle="table.maf_lang_iso639_1">
			<input name="code" message="code" type="text"></input>
			<input name="code2" message="code2" type="text"></input>
			<input name="allnames" message="allnames" type="text"></input>
			<input name="active_yn" bundle="table._common" message="field.active_yn" type="text"></input>
			<input name="local_name" message="local_name" type="text"></input>
			<input name="reg_nm" message="reg_nm" type="text" bundle="common"></input>
		</form>
	</forms>
</action>
	
<action uri="ResourceManager.do" type="miraenet.app.resource.ResourceManagerAction" desc="다국어 메시지관리"
    jsppath="/WEB-INF/jsp/mafapp/resourcemanager/" default="list">
	<action-view>
		<view-info name="list" uri="list.jsp" useTemplate="www" />
		<view-info name="view" uri="view.jsp" useTemplate="www" />
		<view-info name="excel" uri="/WEB-INF/reports/excel/excel_resource.xml" type="excel" />
	</action-view>
	<cmds>
		<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
		<cmd name="view" method="cmdView" formname="frm1" authtype="R" />
		<cmd name="update" method="cmdUpdate" formname="frm1" authtype="U" />
		<cmd name="updateOne" method="cmdUpdateOne" formname="frm1" authtype="U" />
		<cmd name="downloadExcel" method="cmdDownloadExcel" formname="frm1" authtype="R" />
		<cmd name="upExcel" method="cmdUploadExcel" formname="frm1" authtype="U" />
	</cmds>
	<forms>
		<form name="frm1" bundle="table.maf_lang_iso639_1">
			<input name="code" message="code" type="text"></input>
			<input name="code2" message="code2" type="text"></input>
			<input name="allnames" message="allnames" type="text"></input>
			<input name="active_yn" bundle="table._common" message="field.active_yn" type="text"></input>
			<input name="local_name" message="local_name" type="text"></input>
			<input name="reg_nm" message="reg_nm" type="text" bundle="common"></input>
		</form>
	</forms>
</action>
	
<action title="접속통계" uri="webstat.do" type="miraenet.app.webstat.WebStat"
    default="default" jsppath="/WEB-INF/jsp/mafapp/webstat/stat/">
    <action-view>
        <view-info name="default" title="기본" uri="default.jsp" useTemplate="www" />
        <view-info name="fusionxml" title="기본" uri="fusionXmlData.jsp" type="xml" />
        <view-info name="user_log_day" uri="user_log_day.jsp" />
        <view-info name="byOrgType" uri="byOrgType.jsp" useTemplate="www" />
    </action-view>
    <cmds>
        <cmd name="default" method="cmdDefault"  />
        <cmd name="login" method="cmdDefault"  />
        <cmd name="firsthit" method="cmdDefault"  />
        <cmd name="data" method="cmdData"  />
        <cmd name="user_log_day" method="user_log_day" cmdtype="jsp" />
        <cmd name="byOrgType" method="cmdByOrgType"  />
    </cmds>
</action>

<action title="회원통계" uri="memberstat.do" type="miraenet.app.webstat.MemberStat"
    default="age" jsppath="/WEB-INF/jsp/mafapp/webstat/member/">
    <action-view>
        <view-info name="age" uri="age.jsp" useTemplate="www" />
        <view-info name="sex" uri="sex.jsp" useTemplate="www" />
        <view-info name="addr" uri="addr.jsp" useTemplate="www" />
        <view-info name="geoip" uri="geoip.jsp" useTemplate="www" />
    </action-view>
    <cmds>
        <cmd name="age" method="cmdAge" />
        <cmd name="sex" method="cmdSex" />
        <cmd name="addr" method="cmdAddr" />
        <cmd name="geoip" method="cmdGeoip" />
    </cmds>
</action>

</configuration>