    <action uri="SellerMng.do" type="modules.golf.admin.member.SellerMngAction" desc="판매업체관리" title="판매업체관리"
        jsppath="/WEB-INF/jsp/golf/admin/seller/" default="list">
        <action-view>
            <view-info name="list" uri="list.jsp" useTemplate="www" />
            <view-info name="edit" uri="edit.jsp" useTemplate="www" />
            <view-info name="add" uri="edit.jsp" useTemplate="www" />
            <view-info name="search" uri="search.jsp" useTemplate="popup" />
        </action-view>
        <cmds>
            <cmd name="list" method="cmdList" formname="frm1" authtype="R" />
            <cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
            <cmd name="update" method="cmdUpdate" formname="frm1" authtype="U" />
            <cmd name="insert" method="cmdInsert" formname="frm1" authtype="C" />
            <cmd name="add" method="cmdAdd" formname="frm1" authtype="C" />
            <cmd name="delete" method="cmdDelete" formname="frm1" authtype="D" />
            <cmd name="search" method="cmdSearch" formname="frm1" authtype="R" />
        </cmds>
        <forms>
            <form name="frm1" bundle="product">
                <input name="sell_cd" message="sell_cd" type="text" required="true"/>
                <input name="sell_nm" message="sell_nm" type="text" />
                <input name="sell_phone" message="sell_phone" type="text" />
                <input name="sell_fax" message="sell_fax" type="text" />
