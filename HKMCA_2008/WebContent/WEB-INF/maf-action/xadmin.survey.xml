<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-command PUBLIC
         "-//UBQ//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
<global-view></global-view>
    
<action uri="BankMng.do" type="modules.xadmin.survey.BankManagerAction" desc="설문문제은행" title="설문문제은행"
   	jsppath="/WEB-INF/jsp/xadmin/survey/bankmng/" default="list" debug="true">
	<action-view>
		<view-info name="list" uri="list.jsp" useTemplate="www" />
		<view-info name="view" uri="view.jsp" useTemplate="www" />
		<view-info name="edit" uri="edit.jsp" useTemplate="www" />
		<view-info name="add" uri="edit.jsp" useTemplate="www" />
	</action-view>
	<cmds>
		<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
		<cmd name="view" method="cmdView" formname="frm1" authtype="R"  />
		<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U"  />
		<cmd name="add" method="cmdAdd" formname="frm1" authtype="C"  />
		<cmd name="update" method="cmdUpdate" formname="frm1" authtype="U"  />
		<cmd name="insert" method="cmdInsert" formname="frm1" authtype="C"  />
		<cmd name="delete" method="cmdDelete" formname="frm1" authtype="D"  />
	</cmds>
       <forms>
           <form name="frm1" bundle="survey">
               <input name="queid" message="queid" type="text"></input>
               <input name="quetitle" message="quetitle" type="text"></input>
               <input name="quetype" message="quetype" type="text"></input>
               <input name="active_yn" message="active_yn" type="text" bundle="common"></input>
               <input name="upt_dt" message="upt_dt" type="text" bundle="common"></input>
           </form>
       </forms>
</action>
<action uri="SetMng.do" type="modules.xadmin.survey.SetManagerAction" desc="설문세트관리" title="설문세트관리"
   jsppath="/WEB-INF/jsp/xadmin/survey/setmng/" default="list">
	<action-view>
		<view-info name="list" uri="list.jsp" useTemplate="www" />
		<view-info name="edit" uri="edit.jsp" useTemplate="www" />
		<view-info name="add" uri="add.jsp" useTemplate="www" />
	</action-view>
	<cmds>
		<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
		<cmd name="add" method="cmdAdd" formname="frm1" authtype="C" />
        <cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
		<cmd name="insert" method="cmdInsert" />
		<cmd name="update" method="cmdUpdate" />
        <cmd name="updateItems" method="cmdUpdateItems" />
		<cmd name="delete" method="cmdDelete" />
	</cmds>
       <forms>
           <form name="frm1" bundle="survey">
               <input name="settitle" message="settitle" type="text"></input>
               <input name="setdesc" message="setdesc" type="text"></input>
               <input name="quetitle" message="quetitle" type="text"></input>
               <input name="active_yn" message="isuse" type="text" bundle="common"></input>
               <input name="upt_dt" message="upt_dt" type="text" bundle="common"></input>
           </form>
       </forms>
</action>
<action uri="SurveyMng.do" type="modules.xadmin.survey.SurveyManagerAction" title="설문관리" desc="설문관리"
   jsppath="/WEB-INF/jsp/xadmin/survey/surveymng/" default="list">
	<action-view>
		<view-info name="list" uri="list.jsp" useTemplate="www" />
		<view-info name="add" uri="edit.jsp" useTemplate="www" />
		<view-info name="edit" uri="edit.jsp" useTemplate="www" />
        <view-info name="view" uri="view.jsp" useTemplate="www" />
	</action-view>
	<cmds>
		<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
        <cmd name="view" method="cmdView" formname="frm1" authtype="R" />
		<cmd name="add" method="cmdAdd" formname="frm1" authtype="C" />
		<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
		<cmd name="update" method="cmdUpdate" authtype="U" />
		<cmd name="insert" method="cmdInsert" authtype="C" />
		<cmd name="delete" method="cmdDelete" authtype="D" />
	</cmds>
    <forms>
        <form name="frm1" bundle="survey">
            <input name="surveyid" message="surveyid" type="text"></input>
            <input name="surveytitle" message="surveytitle" type="text"></input>
            <input name="surveydesc" message="surveydesc" type="text"></input>
            <input name="set" message="set" type="text" />
            <input name="survey_target" message="survey_target" type="text" />
            <input name="survey_sdat" message="survey_sdat" type="text"></input>
            <input name="survey_edat" message="survey_edat" type="text"></input>
            <input name="upt_dt" message="upt_dt" type="text" bundle="common"></input>
            <input name="active_yn" message="active_yn" type="text" bundle="common"></input>
            <input name="userid" message="userid" type="text" bundle="table.jmf_user"></input>
            <input name="nm" message="nm" type="text" bundle="table.jmf_user"></input>
        </form>
    </forms>
</action>
<action uri="setchoose.do" type="modules.xadmin.survey.SetChooseAction" desc="설문 Set 선택" title="설문 Set 선택"
    jsppath="/WEB-INF/jsp/xadmin/survey/setchoose/" default="list">
    <action-view>
        <view-info name="list" uri="list.jsp" useTemplate="popup" />
        <view-info name="view" uri="view.jsp" useTemplate="popup" />
    </action-view>
    <cmds>
        <cmd name="list" method="cmdList" formname="frm1" authtype="R" />
        <cmd name="view" method="cmdView" formname="frm1" authtype="R" />
    </cmds>
    <forms>
        <form name="frm1" bundle="survey">
            <input name="setid" message="setid" type="text" />
            <input name="setowner" message="setowner" type="text" />
            <input name="settitle" message="settitle" type="text" />
            <input name="setdesc" message="setdesc" type="text" />
            <input name="queid" message="queid" type="text" />
            <input name="quetitle" message="quetitle" type="text" />
            <input name="quescore" message="quescore" type="text" />
            <input name="quelevel" message="quelevel" type="text" />
            <input name="active_yn" bundle="common" message="active_yn" type="text" />
        </form>
    </forms>
</action>
	
<action uri="pollAdmin.do" type="modules.xadmin.poll.PollAdminAction" desc="온라인폴 관리자" title="온라인폴 관리자"
    jsppath="/WEB-INF/jsp/xadmin/poll/" default="list">
    <action-view>
        <view-info name="list" uri="list.jsp" useTemplate="www" />
        <view-info name="edit" uri="edit.jsp" useTemplate="www" />
        <view-info name="add" uri="add.jsp" useTemplate="www" />
        <view-info name="result" uri="/WEB-INF/jsp/poll/result.jsp" useTemplate="popup" />
    </action-view>
    <cmds>
        <cmd name="list" method="cmdList" formname="frm1" authtype="R" />
        <cmd name="add" method="cmdAdd" formname="frm1" from="update" authtype="C" />
        <cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
        <cmd name="insert" method="cmdInsert" formname="frm1" from="update" authtype="C" />
        <cmd name="update" method="cmdUpdate" formname="frm1" from="edit" authtype="U" />
        <cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D" />
        <cmd name="addItem" method="cmdAddItem" formname="frm1" from="update" authtype="C" />
        <cmd name="viewResult" method="cmdViewResult" formname="frm1" from="delete" authtype="R" />
    </cmds>
    <forms>
        <form name="frm1" bundle="survey">
            <input name="site" message="site" type="text"/>
            <input name="poll_id" message="poll_id" type="text" />
            <input name="start_dt" message="start_dt" type="text" />                
            <input name="end_dt" message="end_dt" type="text" />
            <input name="is_use" message="is_use" type="text" />
            <input name="title" message="title" type="text" />
            <input name="seq" message="seq" type="text" />              
            <input name="question" message="question" type="text" />                
            <input name="img" message="img" type="text" />
            <input name="hit" message="hit" type="text" />
            <input name="is_show" message="is_show" type="text" />
        </form>     
    </forms>
</action>

</configuration>