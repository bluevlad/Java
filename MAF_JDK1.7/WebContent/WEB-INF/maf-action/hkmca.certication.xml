<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-resource PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>

	<global-view>
	</global-view>

	<action uri="certiMst.do" type="modules.hkmca.certication.CertiMstMngAction" desc="인증관리" title="인증관리" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/hkmca/certication/certi_mst/list.jsp" useTemplate="www" />
			<view-info name="edit" uri="/WEB-INF/jsp/hkmca/certication/certi_mst/edit.jsp" useTemplate="www" />
			<view-info name="add" uri="/WEB-INF/jsp/hkmca/certication/certi_mst/add.jsp" useTemplate="www" />
			<view-info name="level" uri="/WEB-INF/jsp/hkmca/certication/certi_mst/level.jsp" useTemplate="www" />
		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R"/>
			<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U"/>
			<cmd name="level" method="cmdLevel" formname="frm1" authtype="U"/>
			<cmd name="update" method="cmdUpdate" formname="frm1" from="edit" authtype="U"/>
			<cmd name="insert" method="cmdInsert" formname="frm1" from="update" authtype="C"/>
			<cmd name="add" method="cmdAdd" formname="frm1" from="update" authtype="C"/>
			<cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D"/>
			<cmd name="ins_sec" method="cmdInsSec" formname="frm1" authtype="C"/>
			<cmd name="up_level" method="cmdUpLvl" formname="frm1" authtype="U"/>
		</cmds>
		<forms>
			<form name="frm1" bundle="table.bas_certi">
				<input name="certi_id" message="certi_id" type="text" required="true"/>
				<input name="certi_name" message="certi_name" type="text" />
				<input name="section_id" message="section_id" type="text" required="true"/>
				<input name="description" message="description" type="text" />
				<input name="jasper_id" message="jasper_id" type="text" />
				<input name="isuse" message="isuse" type="text" />
				<input name="section_id" message="section_id" type="text" required="true"/>
				<input name="mst_section" message="mst_section" type="text" />
				<input name="mst_sec_name" message="mst_sec_name" type="text" />
				<input name="grade" message="grade" type="text" />
				<input name="grade_name" message="grade_name" type="text" />
				<input name="sub_section" message="sub_section" type="text" />
				<input name="sub_sec_name" message="sub_sec_name" type="text" />
				<input name="input" message="certi.input" type="text" bundle="common"/>
				<input name="new" message="certi.new" type="text" bundle="common" />
				<input name="code" message="certi.code" type="text" bundle="common" />
				<input name="name" message="certi.name" type="text" bundle="common" />
			</form>		
		</forms>
	</action>

	<action uri="certi.do" type="modules.hkmca.certication.CertiMngAction" desc="인증발급관리" title="인증발급관리" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/hkmca/certication/certi/list.jsp" useTemplate="www" />
			<view-info name="view" uri="/WEB-INF/jsp/hkmca/certication/certi/view.jsp" useTemplate="www" />
			<view-info name="upload" uri="/WEB-INF/jsp/hkmca/certication/certi/upload.jsp" useTemplate="www" />
			<view-info name="jpdf" uri="/WEB-INF/reports/certi/certi_service_advisor_en.jasper" type="jpdf" />
			<view-info name="excel" uri="/WEB-INF/reports/excel/excel_certi.xml" type="excel" />
		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R"/>
			<cmd name="view" method="cmdView" formname="frm1" authtype="R"/>
			<cmd name="update" method="cmdUpdate" formname="frm1" authtype="U" from="edit"/>
			<cmd name="sample" method="cmdSample" formname="frm1" authtype="R" />
			<cmd name="upload" method="cmdUpload" authtype="C" />
			<cmd name="upload_ok" method="cmdUploadOk" formname="frm1" from="update" authtype="C" />
			<cmd name="printCerti" method="printCerti" formname="frm1" authtype="R" />
			<cmd name="excel" method="cmdExcel" formname="frm1" authtype="R" />
		</cmds>
		<forms>
			<form name="frm1" bundle="table.bas_certi">
				<input name="usn" message="table.id" type="text" bundle="common"></input>
				<input name="certi_no" message="certi_no" type="text" ><constraint validatorName="required"/></input>
				<input name="mst_section" message="mst_section" type="text" ></input>
				<input name="grade" message="grade" type="text" ></input>
				<input name="gradeh" message="gradeh" type="text" ></input>
				<input name="certi_date" message="certi_date" type="text" ></input>
				<input name="certi_st" message="certi_st" type="text" ></input>
				<input name="appl_id" message="appl_id" type="text" ></input>
				<input name="appl_dt" message="appl_dt" type="text" ></input>
				<input name="appr_id" message="appr_id" type="text" ></input>
				<input name="appr_dt" message="appr_dt" type="text" ></input>
				<input name="insert_dt" message="insert_dt" type="text" ></input>
				<input name="insert_id" message="insert_id" type="text" ></input>
				<input name="update_dt" message="update_dt" type="text" ></input>
				<input name="update_id" message="update_id" type="text" ></input>
				<input name="cnt" message="cnt" type="text" ></input>
				<input name="user_nm" message="table.name" type="text" bundle="common"></input>
				<input name="sub_section" message="sub_section" type="text" bundle="table.maf_user" ></input>
				<input name="otcc" message="otcc" type="text" ></input>
				<input name="otcn" message="otcn" type="text" ></input>
				<input name="disc" message="disc" type="text" ></input>
				<input name="disn" message="disn" type="text" ></input>
				<input name="dealc" message="dealc" type="text" ></input>
				<input name="dealn" message="dealn" type="text" ></input>
				<input name="userid" message="userid" type="text" ></input>
				<input name="nm" message="usernm" type="text" ></input>
				<input name="email" message="email" type="text" ></input>
				<input name="lan" message="lan" type="text" ></input>
				<input name="certi" message="certi" type="text" ></input>
				<input name="crtno" message="crtno" type="text" ></input>
				<input name="print" message="title.item.print" type="text" bundle="common" />
				<input name="leccode" message="leccode" type="text" bundle="table.bas_class" />
				<input name="lecname" message="lecname" type="text" bundle="table.bas_class" ></input>
				<input name="lec_date" message="lec_date" type="text" bundle="table.bas_class" ></input>
				<input name="lecstat" message="lecstat" type="text"  bundle="table.bas_class"></input>
				<input name="section" message="section" type="text" bundle="table.bas_class" ></input>
				<input name="cert_lvl" message="cert_lvl" type="text" bundle="table.bas_class" ></input>
				<input name="field" message="field" type="text" bundle="table.bas_class" ></input>
				<input name="req_stat" message="req_stat" type="text" bundle="table.wcc_lec_req" ></input>
				<input name="co_attendhours" message="co_attendhours" type="text" bundle="table.wcc_lec_req" ></input>
				<input name="co_attendrate" message="co_attendrate" type="text" bundle="table.wcc_lec_req" ></input>
				<input name="tot_score" message="tot_score" type="text" bundle="table.wce_fnl_grd" ></input>
				<input name="is_grad" message="is_grad" type="text" bundle="table.wcc_lec_req" ></input>
			</form>		
		</forms>
	</action>
</configuration>
	