<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-resource PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
	<template-list>
		<template-info name="inner" uri="/web_src/intranet/_inner.jsp" />
		<template-info name="inner_frame"
			uri="/web_src/intranet/_inner_frame.jsp" />
	</template-list>

	<global-view>
	
	</global-view>
	<action uri="poll.do" type="miraenet.app.poll.PollAction" default="poll">
		<action-view>
			<view-info name="result" uri="/WEB-INF/jsp/mafapp/poll/result.jsp"/>
		</action-view>
		<cmds>
			<cmd name="poll" method="cmdPoll" formname="frm1" authtype="R" />
			<cmd name="viewResult" method="cmdViewResult" formname="frm1" from="delete" authtype="R" />
		</cmds>		
	</action>
	<action uri="pollAdmin.do" type="miraenet.app.poll.PollAdminAction" desc="온라인폴 관리자" title="온라인폴 관리자" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/mafapp/poll/list.jsp" useTemplate="inner" />
			<view-info name="view" uri="/WEB-INF/jsp/mafapp/poll/view.jsp" useTemplate="inner" />
			<view-info name="edit" uri="/WEB-INF/jsp/mafapp/poll/edit.jsp" useTemplate="inner" />
			<view-info name="add" uri="/WEB-INF/jsp/mafapp/poll/add.jsp" useTemplate="inner" />
			<view-info name="result" uri="/WEB-INF/jsp/mafapp/poll/result.jsp"/>
		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
			<cmd name="view" method="cmdView" formname="frm1" authtype="R" />
			<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />

			<cmd name="update" method="cmdUpdate" formname="frm1" from="edit" authtype="U" />
			<cmd name="insert" method="cmdInsert" formname="frm1" from="update" authtype="C" />
			<cmd name="add" method="cmdAdd" formname="frm1" from="update" authtype="C" />
			<cmd name="addItem" method="cmdAddItem" formname="frm1" from="update" authtype="C" />
			<cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D" />
			<cmd name="viewResult" method="cmdViewResult" formname="frm1" from="delete" authtype="R" />
		</cmds>
		<forms>
			<form name="frm1" bundle="table.Poll">
				
					<input name="site" message="form.site" type="text" >
						<constraint validatorName="required"/>
					</input>
				
					<input name="poll_id" message="form.poll_id" type="text" >
						<constraint validatorName="required"/>
					</input>
				
					<input name="start_dt" message="form.start_dt" type="text" />				
					<input name="end_dt" message="form.end_dt" type="text" />
					<input name="is_use" message="form.is_use" type="text" />
				
					<input name="title" message="form.title" type="text" >
						<constraint validatorName="required"/>
					</input>

					<input name="seq" message="form.seq" type="text" />				
					<input name="question" message="form.question" type="text" />				
					<input name="img" message="form.img" type="text" />
					<input name="hit" message="form.hit" type="text" />
				
					<input name="is_show" message="form.is_show" type="text" >
						
					</input>
				
				
			</form>		
		</forms>
	</action>
	

</configuration>

		