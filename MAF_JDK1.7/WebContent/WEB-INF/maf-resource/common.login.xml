<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE maf-resource PUBLIC "-//Miranet//DTD maf-command config XML V1.0//EN" "resourceBundle.dtd">

<bundle defaultLocale="en">
  <resource key="btn.findid" type="text">
    <message locale="en" value="ID search" />
    <message locale="ko" value="[id찾기]" />
  </resource>
  <resource key="btn.login" type="text">
    <message locale="en" value="Log in" />
    <message locale="ko" value="[LOGIN]" />
  </resource>
  <resource key="btn.register" type="text">
    <message locale="en" value="Membership register" />
    <message locale="ko" value="[회원가입]" />
  </resource>
  <resource key="msg.invalidUserID" type="text">
    <message locale="en" value="Unregistered ID" />
    <message locale="ko" value="{0}은 등록된 ID가 아닙니다." />
  </resource>
  <resource key="msg.invalidlogin" type="text">
    <message locale="en" value="Error in ID and/or password" />
    <message locale="ko" value="아이디 또는 패스워드 오류입니다." desc="현재 입력하신 아이디가 네이버에 등록되어 있지 않거나,아이디 또는 패스워드를 잘못 입력 하셨습니다." />
  </resource>
  <resource key="msg.invalidpassword" type="text">
    <message locale="en" value="Invalid Password" />
    <message locale="ko" value="잘못된 password입니다.." />
  </resource>
  <resource key="msg.loginfail" type="text">
    <message locale="en" value="Error during log in" />
    <message locale="ko" value="Login중 오류가 발생하였습니다." />
  </resource>
  <resource key="removeSession" type="text">
    <message locale="en" value="Disconnect" />
    <message locale="ko" value="기존 접속정보가 제거 되었습니다." />
  </resource>
</bundle>

