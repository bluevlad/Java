<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="multipleResultSetsEnabled" value="true" />
        <setting name="useColumnLabel" value="true" />
        <setting name="useGeneratedKeys" value="false" />
        <setting name="defaultExecutorType" value="SIMPLE" />
        <setting name="defaultStatementTimeout" value="5000" />
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>

    <typeAliases>
        <typeAlias  alias="egovMap"             type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
        <typeAlias  alias="comDefaultVO"       type="com.willbes.cmm.ComDefaultVO"/>
        <typeAlias  alias="comDefaultExtVO"    type="com.willbes.cmm.ComDefaultExtVO"/>
        <typeAlias  alias="ComDefaultCodeVO" type="com.willbes.cmm.ComDefaultCodeVO"/>
        <typeAlias  alias="CmmnDetailCode"    type="com.willbes.cmm.service.CmmnDetailCode"/>
        <typeAlias  alias="FileVO"                 type="com.willbes.cmm.service.FileVO"/>

    </typeAliases>

</configuration>