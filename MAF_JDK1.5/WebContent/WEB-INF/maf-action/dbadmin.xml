<?xml version="1.0" encoding="euc-kr"?>
<!DOCTYPE maf-command PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
	<template-list>
		<template-info name="inner" uri="/WEB-INF/jsp/mafapp/dbadmin/_inner.jsp" />
		<template-info name="contents" uri="/WEB-INF/jsp/mafapp/dbadmin/_contents.jsp" />
		<template-info name="procinfo" uri="/WEB-INF/jsp/mafapp/dbadmin/_procinfo.jsp" />
	</template-list>

	<global-view>
	</global-view>

	<action uri="default.do" 	type="miraenet.app.dbadmin.actions.Default" >
		<action-view>
			<view-info name="default" uri="/WEB-INF/jsp/mafapp/dbadmin/index.jsp"  />

		</action-view>
	</action>
	<action uri="top.do" 	type="miraenet.app.dbadmin.actions.Top">
		<action-view>
			<view-info name="top" uri="/WEB-INF/jsp/mafapp/dbadmin/top.jsp" useTemplate="inner" />
		</action-view>		
	</action>
	<action uri="mssqltable.do" 	type="maf.modules.dbadmin.actions.MSSqlTable">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/mafapp/dbadmin/sqltable.jsp" />
		</action-view>
	</action>
	<action uri="tableList.do" 	type="miraenet.app.dbadmin.actions.TableList">
		<action-view>
			<view-info name="tableList" uri="/WEB-INF/jsp/mafapp/dbadmin/table_list.jsp" useTemplate="inner" />		
		</action-view>	
	</action>
	
	<action uri="tabsinfo.do" 	type="miraenet.app.dbadmin.actions.TableInfo" default="column">
		<action-view>
			
		
			<view-info name="sql" uri="/WEB-INF/jsp/mafapp/dbadmin/sql.jsp" useTemplate="contents" />
			<view-info name="form" uri="/WEB-INF/jsp/mafapp/dbadmin/form.jsp" useTemplate="contents" />
			<view-info name="procInfo" uri="/WEB-INF/jsp/mafapp/dbadmin/proc_info.jsp" useTemplate="procinfo" />
			<view-info name="indexInfo" uri="/WEB-INF/jsp/mafapp/dbadmin/index_info.jsp" useTemplate="contents" />
			<view-info name="javabean" uri="/WEB-INF/jsp/mafapp/dbadmin/java_bean.jsp" useTemplate="contents" />
			<view-info name="javastmt" uri="/WEB-INF/jsp/mafapp/dbadmin/java_stmt.jsp" useTemplate="contents" />	



			<view-info name="JavaAction" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JavaAction.jsp" useTemplate="contents" />
			<view-info name="JavaDB" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JavaDB.jsp" useTemplate="contents" />
			<view-info name="JavaActionConfig" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JavaActionConfig.jsp" useTemplate="contents" />
			<view-info name="JavaResourceConfig" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JavaResourceConfig.jsp" useTemplate="contents" />
			<view-info name="JspList" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JspList.jsp" useTemplate="contents" />
			<view-info name="JspView" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JspView.jsp" useTemplate="contents" />
			<view-info name="JspEdit" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JspEdit.jsp" useTemplate="contents" />
			<view-info name="JspAdd" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/JspAdd.jsp" useTemplate="contents" />
			<view-info name="ExcelUpload" uri="/WEB-INF/jsp/mafapp/dbadmin/tpl01/excelupload.jsp" useTemplate="contents" />
			
		</action-view>
		<cmds>
			<cmd name="sql" method="getSqlInfo"/>
			<cmd name="beans" method="getBeans"/>
			<cmd name="SQL" method="getSQL"/>
			<cmd name="STMT" method="getSTMT"/>
			<cmd name="index" method="getIndex"/>
			<cmd name="excel" method="getExcel"/>
			<cmd name="form" method="getForm"/>

			<cmd name="JavaAction" method="getJavaSource"/>
			<cmd name="JavaDB" method="getJavaSource"/>
			<cmd name="JavaActionConfig" method="getJavaSource"/>
			<cmd name="JavaResourceConfig" method="getJavaSource"/>
			<cmd name="JspList" method="getJavaSource"/>
			<cmd name="JspView" method="getJavaSource"/>
			<cmd name="JspEdit" method="getJavaSource"/>
			<cmd name="JspAdd" method="getJavaSource"/>
			<cmd name="ExcelUpload" method="getJavaSource"/>

		</cmds>
	</action>
	<action uri="procinfo.do" 	type="miraenet.app.dbadmin.actions.ProcInfo" />
	<action uri="dbsyncconfig.do" type="miraenet.app.dbsync.ShowConfig"  title="dbsync정보보기" desc="dbsync정보보기" default="default">
		
		<action-view>
			<view-info  name="default" uri="/WEB-INF/jsp/mafapp/dbsync/dbsyncstatus.jsp" />
		</action-view>
		<cmds>
			<cmd name="default" method="cmdDefault"/>
			<cmd name="start" method="cmdStart"/>
			<cmd name="stop" method="cmdStop"/>
		</cmds>
	</action>	
</configuration>
