	<action uri="MenuAdmin.do" type="miraenet.app.siteManager.MenuAdmin" title="메뉴관리" desc="메뉴관리" default="default" debug="true">
		<action-view>
			<view-info name="default" uri="/WEB-INF/jsp/mafapp/sitemanager/menuadmin/menu_admin.jsp" useTemplate="www" />
			<view-info name="tree" uri="/WEB-INF/jsp/mafapp/sitemanager/menuadmin/menu_tree.jsp" useTemplate="inner_frame" />
			<view-info name="edit" uri="/WEB-INF/jsp/mafapp/sitemanager/menuadmin/menu_edit.jsp" useTemplate="inner_frame" />
