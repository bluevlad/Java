<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE maf-resource PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
	<template-list>
		<template-info name="doTest" uri="/WEB-INF/jsp/etest/learner/dotest/_popup.jsp" />
	</template-list>
	<action uri="doTest.do" type="modules.etest.learner.DoTestAction" desc="시험응시" title="시험응시" default="confirm" debug="true">
		<action-view>
			<view-info name="confirm" uri="/WEB-INF/jsp/etest/learner/dotest/confirm.jsp" useTemplate="doTest" />
			<view-info name="test" uri="/WEB-INF/jsp/etest/learner/dotest/test.jsp" useTemplate="doTest" />
		</action-view>
		<cmds>
			<cmd name="confirm" method="cmdConfirm"  />
			<cmd name="test" method="cmdTest"  />
			<cmd name="updateAnswer" method="cmdUpdateAnswer"  />
			<cmd name="updateMark" method="cmdUpdateMark"  />
			<cmd name="finish" method="cmdFinish"  />
		</cmds>
		<forms>
			<form name="frm1" bundle="table.exm_rst">
				
			</form>
		</forms>
	</action>
	<action uri="examlist.do" type="modules.etest.learner.ExamListAction" desc="시험목록" title="시험목록" default="list" debug="true">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/etest/learner/examlist/list.jsp" useTemplate="www" />
			<view-info name="view" uri="/WEB-INF/jsp/etest/learner/examlist/view.jsp" useTemplate="www" />
			<view-info name="result" uri="/WEB-INF/jsp/etest/learner/examlist/result.jsp" useTemplate="www" />
			<view-info name="resultXml" uri="/WEB-INF/jsp/etest/learner/examlist/resultXml.jsp" />
		</action-view>
		<cmds>
			<cmd name="tree" method="getTree" authtype="R" />
			<cmd name="treeSel" method="getTreeSel" authtype="R" />
			<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
			<cmd name="view" method="cmdView" formname="frm1" authtype="R" />
			<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
			<cmd name="update" method="cmdUpdate" formname="frm1" from="edit" authtype="U" />
			<cmd name="insert" method="cmdInsert" formname="frm1" from="update" authtype="C" />
			<cmd name="add" method="cmdAdd" formname="frm1" from="update" authtype="C" />
			<cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D" />
			<cmd name="result" method="cmdResult" authtype="R" />
			<cmd name="userHistgram" method="resultXml"  authtype="R" cmdtype="jsp"/>
			
		</cmds>
		<forms>
			<form name="frm1" bundle="table.exm_rst">
				<input name="exmid" message="exmid" bundle="table.exm_mst" type="text" required="true" />
				<input name="exmtitle" message="exmtitle" bundle="table.exm_mst" type="text" required="true" />
				<input name="exm_sdat_t1" message="exm_sdat_t1" bundle="table.exm_mst" type="text" required="true" />
				<input name="exm_edat_t1" message="exm_edat_t1" bundle="table.exm_mst" type="text" required="true" />
				<input name="rst_status" message="rst_status" type="text" required="true" />
				<input name="active_yn" message="active_yn" type="text" />
				<input name="reg_usn" bundle="table._common" message="field.reg_usn" type="text" />
				<input name="reg_dt" bundle="table._common" message="field.reg_dt" type="text" />
				<input name="update_dt" bundle="table._common" message="field.update_dt" type="text" />
				<input name="update_usn" bundle="table._common" message="field.update_id" type="text" />
				<input name="active_yn" bundle="common" message="select.use" type="text" />
				<input name="org_name" bundle="table.bas_org" message="org_name" type="text" />
				<input name="user_nm" bundle="table.maf_user" message="nm" type="text" />
				<input name="questions" message="questions" bundle="table.exm_mst" type="text" />
				<input name="lang" message="lang" bundle="common" type="text" />
				<input name="exmtime" message="exmtime" bundle="table.exm_set" type="text" />
			</form>
		</forms>
	</action>
</configuration>