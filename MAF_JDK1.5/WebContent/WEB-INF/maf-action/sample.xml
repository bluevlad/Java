<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-command PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
	<template-list>
		<template-info name="inner" uri="/WEB-INF/jsp/_sample/_style1.jsp" />
	</template-list>

	<global-view>
		<view-info name="default" uri="/WEB-INF/jsp/_sample/index.jsp"  />
	</global-view>

	<action uri="default.do" type="modules.app._sample.Sample1" 
		desc="sample" 
		title="sample title" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/_sample/list.jsp" useTemplate="inner" />
			<view-info name="view" uri="/WEB-INF/jsp/_sample/view.jsp" useTemplate="inner" />
			<view-info name="edit" uri="/WEB-INF/jsp/_sample/edit.jsp" useTemplate="inner" />
			<view-info name="add" uri="/WEB-INF/jsp/_sample/add.jsp" useTemplate="inner" />
		</action-view>	
		<cmds>
			<cmd name="list" method="cmdList" />
			<cmd name="view" method="cmdView" formname="sample"/>
			<cmd name="edit" method="cmdEdit" formname="sample" />
			<cmd name="add" method="cmdAdd" formname="sample" />
			<cmd name="update" method="cmdUpdate" formname="sample" from="edit" authtype="U" />
			<cmd name="insert" method="cmdInsert" from="update" authtype="C"/>
		</cmds>
		<forms>
			<form name="sample" bundle="sample">
				<input name="title" message="form.title" type="text" >
					<constraint validatorName="required"/>
					<constraint validatorName="maxlength" value="10b"/>
					<constraint validatorName="minlength" value="4b"/>
				</input>
				<input name="reg_dt" message="">
				</input>
			</form>
		</forms>		
	</action>
	<action uri="excelupload.do" type="miraenet.app.excelUploader.ExcelUpload" 
		desc="excel Upload" 
		title="sample title" default="default">
		<action-view>
			<view-info name="default" uri="/WEB-INF/jsp/_sample/excelupload/default.jsp" useTemplate="inner" />
			
		</action-view>	
		<cmds>
			<cmd name="default" method="cmdDefault" />
			<cmd name="sample" method="cmdSample" />
		</cmds>
		
	</action>
</configuration>
