		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
			<cmd name="view" method="cmdView" formname="frm1" authtype="R" />
			<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
			<cmd name="update" method="cmdUpdate" formname="frm1" from="edit" authtype="U" />
			<cmd name="updateItems" method="cmdUpdateItems" formname="frm1" from="edit" authtype="U" />
			<cmd name="insert" method="cmdInsert" formname="frm1" from="update" authtype="C" />
			<cmd name="add" method="cmdAdd" formname="frm1" from="update" authtype="C" />
			<cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D" />
			
		</cmds>
		<forms>
			<form name="frm1" bundle="table.survey_set">
				<input name="setid" message="setid" type="text" required="true" />
				<input name="setowner" message="setowner" type="text" required="true" />
				<input name="settitle" message="settitle" type="text" />
				<input name="surveytype" message="surveytype" bundle="table.survey_mst" type="text" />
				<input name="setdesc" message="setdesc" type="text" />
				<input name="reg_usn" bundle="table._common" message="field.reg_usn" type="text" />
				<input name="reg_dt" bundle="table._common" message="field.reg_dt" type="text" />
				<input name="update_dt" bundle="table._common" message="field.update_dt" type="text" />
				<input name="update_usn" bundle="table._common" message="field.update_usn" type="text" />
				<input name="active_yn" bundle="common" message="select.use" type="text" required="true" />
				<input name="queid" message="queid" bundle="table.exm_bank" type="text" />
				<input name="quetitle" message="quetitle" bundle="table.exm_bank" type="text" />
			</form>
		</forms>
