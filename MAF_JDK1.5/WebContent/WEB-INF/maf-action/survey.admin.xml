				<input name="org_name" message="org_name" type="text" bundle="table.bas_org" />
				<input name="setid" message="setid" type="text" bundle="table.survey_set" />
				<input name="setowner" message="setowner" type="text" bundle="table.survey_set" />
				<input name="settitle" message="settitle" type="text" bundle="table.survey_set" />
				<input name="reg_usn" bundle="table._common" message="field.reg_usn" type="text" />
				<input name="reg_dt" bundle="table._common" message="field.reg_dt" type="text" />
				<input name="update_dt" bundle="table._common" message="field.update_dt" type="text" />
				<input name="update_usn" bundle="table._common" message="field.update_usn" type="text" />
				<input name="active_yn" bundle="common" message="select.use" type="text" required="true" />
			</form>
		</forms>
	</action>
	
