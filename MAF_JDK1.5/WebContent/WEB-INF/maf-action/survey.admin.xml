<configuration>
	<global-view></global-view>
	<action uri="bank.do" type="modules.survey.admin.BankManagerAction" desc="설문은행관리" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/survey/admin/bankmanager/list.jsp" useTemplate="www" />
			<view-info name="edit" uri="/WEB-INF/jsp/survey/admin/bankmanager/edit.jsp" useTemplate="www" />
			<view-info name="add" uri="/WEB-INF/jsp/survey/admin/bankmanager/add.jsp" useTemplate="www" />
			<view-info name="editLang" uri="/WEB-INF/jsp/survey/admin/bankmanager/editLang.jsp" useTemplate="www" />
		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
			<cmd name="edit" method="cmdEdit" formname="frm1" authtype="U" />
			<cmd name="update" method="cmdUpdate" formname="frm1" from="edit" authtype="U" />
			<cmd name="insert" method="cmdInsert" formname="frm1" from="update" authtype="C" />
			<cmd name="add" method="cmdAdd" formname="frm1" from="update" authtype="C" />
			<cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D" />
			<cmd name="editLang" method="cmdEditLang" formname="frm1" authtype="U" />
		</cmds>
		<forms>
			<form name="frm1" bundle="table.survey_bank">
				<input name="queid" message="queid" type="text" required="true" />
				<input name="qweowner" message="qweowner" type="text" required="true" />
				<input name="quetitle" message="quetitle" type="text" />
				<input name="quecount" message="quecount" type="text" />
				<input name="quetype" message="quetype" type="text" required="true" />
				<input name="update_dt" bundle="table._common" message="field.update_dt" type="text" />
				<input name="active_yn" bundle="common" message="select.active" type="text" />
			</form>
		</forms>
	</action>
	
	<action uri="setmanager.do" type="modules.survey.admin.SetManagerAction" desc="설문 Set 관리" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/survey/admin/setmanager/list.jsp" useTemplate="www" />
