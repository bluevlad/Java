<?xml version="1.0" encoding="UTF-8"  ?>
<!DOCTYPE maf-command PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "excel-upload.dtd">
<upload desc="upload group certification" startrow="1">
	<sqls>
		<sql seq="0">
			 INSERT INTO BAS_CERTI
			    (usn, certi_no, certi_id, certi_date, insert_dt, insert_id)
			  VALUES
			    (:usn, :certi_no, :certi_id, :certi_date, sysdate, :insert_id)
		</sql>
	</sqls>
	<fields>
		<field col="0" fieldname="usn" title="usn" check="true" sample="2049988" desc="USER ID" />
		<field col="1" fieldname="certi_no" title="certi_no" check="true" sample="SEN-0003" desc="Certi. Issue No." />
		<field col="2" fieldname="certi_id" title="certi_id" check="true" sample="SEN" desc="certificate issue code" />
		<field col="3" fieldname="certi_date" title="certi_date" sample="2007-01-01" desc="certification date" />
		<field col="4" fieldname="insert_id" title="insert_id" sample="admin" desc="User ID of Register" />
	</fields>
</upload>
