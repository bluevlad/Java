<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-command PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "resourceBundle.dtd">
<bundle defaultLocale="en">

	<resource key="userid" >
		<message locale="ko" value="회원id" />
		<message locale="en" value="Userid" />
	</resource>

	<resource key="send_no" >
		<message locale="ko" value="발송일련번호" />
		<message locale="en" value="Mail Sequence ID" />
	</resource>

	<resource key="send_id" >
		<message locale="ko" value="발송id" />
		<message locale="en" value="Sender ID" />
	</resource>

	<resource key="leccode" >
		<message locale="ko" value="강의코드" />
		<message locale="en" value="Lecture Code" />
	</resource>

	<resource key="user_nm" >
		<message locale="ko" value="회원명" />
		<message locale="en" value="Username" />
	</resource>

	<resource key="email" >
		<message locale="ko" value="이메일" />
		<message locale="en" value="Email" />
	</resource>

	<resource key="result" >
		<message locale="ko" value="전송결과[0:전송성공,  0 미만 값: 전송실패]" />
		<message locale="en" value="Result" />
	</resource>

	<resource key="from_id" >
		<message locale="ko" value="보낸이" />
		<message locale="en" value="Sender" />
	</resource>

	<resource key="send_dt" >
		<message locale="ko" value="보낸일" />
		<message locale="en" value="Sent Date" />
	</resource>

	<resource key="mail_title" >
		<message locale="ko" value="메일제목" />
		<message locale="en" value="Subject" />
	</resource>

	<resource key="mail_text" >
		<message locale="ko" value="메일내용" />
		<message locale="en" value="Content" />
	</resource>

	<resource key="mail_type" >
		<message locale="ko" value="메일형태[a:경고[독촉]/ i:알림메일]" />
		<message locale="en" value="Mail Type" />
	</resource>

	<resource key="update_dt" >
		<message locale="ko" value="최종수정일[수정된 현재일이 등록됨]" />
		<message locale="en" value="Update Date" />
	</resource>

	<resource key="update_id" >
		<message locale="ko" value="최종수정id" />
		<message locale="en" value="Update ID" />
	</resource>

	<resource key="mail_pattern" >
		<message locale="ko" value="메일 내용 유형" />
		<message locale="en" value="Mail Type" />
	</resource>

	<resource key="isread" >
		<message locale="ko" value="읽음 여부(y:읽음, n:안읽음)" />
		<message locale="en" value="Read" />
	</resource>
	
	<resource key="select.wse_mail_history.mail_type.alert" >
		<message locale="ko" value="경고[Alert]메일"/>
		<message locale="en" value="Alert Mail"/>
	</resource>
	<resource key="select.wse_mail_history.mail_type.info" >
		<message locale="ko" value="알림[Information]메일"/>
		<message locale="en" value="Information Mail"/>
	</resource>
	<resource key="select.wse_mail_history.result.ok" >
		<message locale="ko" value="성공"/>
		<message locale="en" value="Success"/>
	</resource>
	<resource key="select.wse_mail_history.result.error1" >
		<message locale="ko" value="메일주소오류"/>
		<message locale="en" value="Invalid Email Address"/>
	</resource>
	<resource key="select.wse_mail_history.result.error2" >
		<message locale="ko" value="전송실패"/>
		<message locale="en" value="Failure"/>
	</resource>
	<resource key="select.wse_mail_history.result.error3" >
		<message locale="ko" value="실패[기타오류]"/>
		<message locale="en" value="Failure (ETC)"/>
	</resource>
	<resource key="select.wse_mail_history.result.error" >
		<message locale="ko" value="실패"/>
		<message locale="en" value="Failure"/>
	</resource>
	
</bundle>	

