	<tag>
		<name>submit</name>
		<tag-class>maf.web.tags.form.MafSubmitTag</tag-class>
		<body-content>JSP</body-content>
		<description>Maps key to localized message and performs parametric replacement</description>
		<attribute>
			<name>key</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Message key to be looked up.</description>
		</attribute>
		<attribute>
			<name>bundle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Localization context in whose resource bundle the message key is looked up.</description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>input attribute name</description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>input attribute value</description>
		</attribute>
		<attribute>
			<name>icon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>input attribute value</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<description>input attribute type ( RESET| 이 아니면 내부에서 처리 SUBMIT|IMAGE) BUTTON은 HTML3.2 표준 Type이 아님 )</description>
		</attribute>
		<attribute>
			<name>locale</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>locale of var.(for debuging)</description>
		</attribute>
		<!-- <dynamic-attributes>true</dynamic-attributes>-->
	</tag>
	<tag>
		<name>button</name>
		<tag-class>maf.web.tags.form.MafButtonTag</tag-class>
		<body-content>JSP</body-content>
		<description>Maps key to localized message and performs parametric replacement</description>
		<attribute>
			<name>key</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Message key to be looked up.</description>
		</attribute>
		<attribute>
			<name>bundle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Localization context in whose resource bundle the message key is looked up.</description>
		</attribute>
		<attribute>
			<name>var</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Name of the exported scoped variable which stores the localized message.</description>
		</attribute>
		<attribute>
			<name>scope</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>Scope of var.</description>
		</attribute>
		<attribute>
			<name>param</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				Supplies an argument for parametric replacement to a containing &lt;message&gt; tag, Object, Object[], Colletion 가능
			</description>
		</attribute>
		<attribute>
			<name>locale</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>locale of var.(for debuging)</description>
		</attribute>
		<attribute>
			<name>icon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>input attribute value</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>onClick</description>
		</attribute>
		<attribute>
			<name>href</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>href url</description>
		</attribute>
	</tag>
	<tag>
		<name>label</name>
		<tag-class>maf.web.tags.form.MafLabelTag</tag-class>
		<body-content>empty</body-content>
		<description>label을 그려준다.(required조건시 해당 style class 표시)</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>field Name</description>
		</attribute>
		<attribute>
			<name>for</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>for attribute</description>
		</attribute>
		<attribute>
			<name>remark</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>desription</description>
		</attribute>		
	</tag>
	<tag>
		<name>header</name>
		<tag-class>maf.web.tags.form.MafHeaderTag</tag-class>
