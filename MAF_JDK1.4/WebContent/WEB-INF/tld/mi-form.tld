			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>field Name</description>
		</attribute>
		<attribute>
			<name>sort</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>sort 기능 부여 여부 (true|false) </description>
		</attribute>
		<attribute>
			<name>remark</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>desription</description>
		</attribute>		
