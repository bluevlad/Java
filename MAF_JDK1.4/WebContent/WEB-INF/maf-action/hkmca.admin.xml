			</form>		
		</forms>
	</action>
	
	
	<action uri="orgProfileCat.do" type="modules.hkmca.admin.OrgProfileCatAction" desc="프로필 카테고리관리"
		title="프로필 카테고리관리" default="default" debug="true" jsppath="/WEB-INF/jsp/hkmca/admin/orgProfileCat">
		<action-view>
			<view-info name="default" uri="default.jsp" useTemplate="www" />
			<view-info name="edit" uri="edit.jsp" useTemplate="inner_frame" />
