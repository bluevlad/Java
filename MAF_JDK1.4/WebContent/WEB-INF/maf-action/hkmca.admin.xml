				<input name="reg_dt" bundle="table._common" message="field.reg_dt" type="text" />
				<input name="update_dt" bundle="table._common" message="field.update_dt" type="text" />
				<input name="update_usn" bundle="table._common" message="field.update_id" type="text" />
				<input name="active_yn" bundle="common" message="select.use" type="text" />
				<input name="seq" message="field.seq" bundle="table._common" type="text" />
			</form>
		</forms>
	</action>
