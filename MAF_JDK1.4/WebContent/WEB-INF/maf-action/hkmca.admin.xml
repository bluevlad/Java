	</global-view>
	
	<action uri="org.do" type="modules.hkmca.admin.OrgAction" desc="Organization" title="Organization" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/hkmca/admin/org/list.jsp" useTemplate="www" />
		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R"/>
		</cmds>
		<forms>
			<form name="frm1" bundle="table.bas_org">
				<input name="org_cd" message="org_cd" type="text" required="true"/>
				<input name="p_org_cd" message="p_org_cd" type="text" />
