			<cmd name="delete" method="cmdDelete" formname="frm1" from="delete" authtype="D" />
		</cmds>
		<forms>
			<form name="frm1" bundle="table.exm_category">
				<input name="cat_id" message="cat_id" type="text" required="true" />
