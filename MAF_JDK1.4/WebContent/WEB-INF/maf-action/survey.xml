<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE maf-resource PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "mvc-configuration.dtd">
<configuration>
	<global-view></global-view>
	<action uri="survey.do" type="modules.survey.SurveyAction" desc="설문참여" default="list">
		<action-view>
			<view-info name="list" uri="/WEB-INF/jsp/survey/survey/list.jsp" useTemplate="www" />
			<view-info name="view" uri="/WEB-INF/jsp/survey/survey/view.jsp" useTemplate="www" />
		</action-view>
		<cmds>
			<cmd name="list" method="cmdList" formname="frm1" authtype="R" />
			<cmd name="view" method="cmdView" formname="frm1" authtype="R" />
			<cmd name="survey" method="cmdSurvey" formname="frm1" from="update" authtype="U" />
		</cmds>
		<forms>
			<form name="frm1" bundle="table.survey_mst">
				<input name="surveyid" message="surveyid" type="text" required="true" />
				<input name="surveytitle" message="surveytitle" type="text" />
				<input name="surveydesc" message="surveydesc" type="text" />
				<input name="survey_sdat_t1" message="survey_sdat_t1" type="text" />
				<input name="survey_edat_t1" message="survey_edat_t1" type="text" />
				<input name="active_yn" bundle="common" message="select.use" type="text" required="true" />
			</form>
		</forms>
	</action>
</configuration>
