<?xml version="1.0" encoding="UTF-8"  ?>
<!DOCTYPE maf-command PUBLIC
         "-//Miranet//DTD maf-command config XML V1.0//EN"
         "excel-upload.dtd">
<upload desc="upload onsite 강좌등록" startrow="1">
	<sqls>
		<sql seq="0">
			{call SP_INS_CLASS(:sjt_cd, :lec_year, :lecname, :lec_sdate, :lec_edate, :orgn_cd, :update_id)}
		</sql>
	</sqls>
	<fields>
		<field col="0" fieldname="sjt_cd" title="sjt_cd" check="true" sample="CRTY-SAL-1-1" desc="Lecture Code" />
		<field col="1" fieldname="lec_year" title="lec_year" sample="2007" desc="Lecture Year" />
		<field col="2" fieldname="lecname" title="lecname" sample="Sales Step 1" desc="Lecture Name" />
		<field col="3" fieldname="lec_sdate" title="lec_sdate" sample="20070101" desc="Start Date(YYYYMMDD)" />
		<field col="4" fieldname="lec_edate" title="lec_edate" sample="20070130" desc="Finish Date(YYYYMMDD)" />
	</fields>
</upload>